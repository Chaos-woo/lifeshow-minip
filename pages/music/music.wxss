/* pages/capture/capture.wxss */
@font-face {
  font-family: "iconfont";
  src: url("//at.alicdn.com/t/font_1704620_35z6ohfksgq.eot?t=1586497993249"); /* IE9 */
  src: url("//at.alicdn.com/t/font_1704620_35z6ohfksgq.eot?t=1586497993249#iefix")
      format("embedded-opentype"),
    /* IE6-IE8 */
      url("data:application/x-font-woff2;charset=utf-8;base64,")
      format("woff2"),
    url("//at.alicdn.com/t/font_1704620_35z6ohfksgq.woff?t=1586497993249")
      format("woff"),
    url("//at.alicdn.com/t/font_1704620_35z6ohfksgq.ttf?t=1586497993249")
      format("truetype"),
    /* chrome, firefox, opera, Safari, Android, iOS 4.2+ */
      url("//at.alicdn.com/t/font_1704620_35z6ohfksgq.svg?t=1586497993249#iconfont")
      format("svg"); /* iOS 4.1- */
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 20px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-play:before {
  content: "\e63a";
}

.icon-tips:before {
  content: "\e66d";
}

page {
  width: 100%;
  height: 100%;
  position: relative;
}

.audio {
  position: relative;
}

.audio image {
  width: 100%;
  height: 170px;
}

.player {
  background-color: rgba(255, 255, 255, 0.6);
  width: 650rpx;
  height: 130px;
  position: absolute;
  bottom: 20px;
  left: 50rpx;
  border-radius: 10rpx;
  display: flex;
  flex-direction: column;
  justify-content: flex-end;
}

.info {
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  color: #000000;
}

.info view {
  margin-bottom: 10px;
}

.gap {
  width: 100%;
  height: 45px;
  background-color: #ffdd94;
  font-size: 17px;
  color: #fff;
  font-weight: bold;
  text-align: center;
  line-height: 45px;
  position: relative;
}

.gap .tip {
  position: absolute;
  right: 0;
  bottom: 0;
  color: #000;
  font-size: 15px;
  width: 100rpx;
  height: 100%;
}

.item {
  width: 100%;
  height: 40px;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  margin-bottom: 5px;
}

.commons {
  height: 40px;
  line-height: 40px;
}

.index {
  width: 15%;
  text-align: center;
}

.options {
  width: 20%;
  text-align: center;
  color: gray;
}

.song-info {
  width: 65%;
  height: 40px;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: flex-start;
}

.song-info .name {
  width: 100%;
  flex: 3;
  margin-top: 2px;
  display: -webkit-box;
  overflow: hidden;
  white-space: normal;
  text-overflow: ellipsis;
  word-wrap: break-word;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}

.song-info .singer {
  width: 100%;
  flex: 2;
  color: gray;
  font-size: 13px;
}

.nav-bar-search {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
  -ms-flex-pack: center;
  justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
  -ms-flex-align: center;
  align-items: center;
  width: 100%;
  /* prettier-ignore */
  height: 32PX;
  /* prettier-ignore */
  font-size: 16PX;
  /* prettier-ignore */
  border-radius: 16PX;
  /* prettier-ignore */
  padding-left: 5PX;
  position: relative;
  background: rgb(198, 221, 216);
}

.srch-ipt {
  /* prettier-ignore */
  height: 32PX;
  text-align: left;
  /* prettier-ignore */
  font-size: 15PX;
}

.nav-bar-search_input {
  flex: 1;
  color: #000000;
}

.movable-view image {
  width: 100%;
}

.nav-bar-search_icon {
  /* prettier-ignore */
  width: 22PX;
  /* prettier-ignore */
  height: 22PX;
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  -ms-flex-align: center;
  align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
  -ms-flex-pack: center;
  justify-content: center;
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-size: cover;
}
