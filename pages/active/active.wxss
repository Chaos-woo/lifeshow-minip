/* pages/active/active.wxss */
@font-face {font-family: "iconfont";
  src: url('//at.alicdn.com/t/font_1704620_3no7va44q7n.eot?t=1585281404423'); /* IE9 */
  src: url('//at.alicdn.com/t/font_1704620_3no7va44q7n.eot?t=1585281404423#iefix') format('embedded-opentype'), /* IE6-IE8 */
  url('data:application/x-font-woff2;charset=utf-8;base64,') format('woff2'),
  url('//at.alicdn.com/t/font_1704620_3no7va44q7n.woff?t=1585281404423') format('woff'),
  url('//at.alicdn.com/t/font_1704620_3no7va44q7n.ttf?t=1585281404423') format('truetype'), /* chrome, firefox, opera, Safari, Android, iOS 4.2+ */
  url('//at.alicdn.com/t/font_1704620_3no7va44q7n.svg?t=1585281404423#iconfont') format('svg'); /* iOS 4.1- */
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-fire:before {
  content: "\e600";
  color:rgb(255, 0, 0)
}

page {
  width: 100%;
  height: 100%;
}

.scroll-panel{
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: center;
}

.activ-nav{
  width: 100%;
  height: 60rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  padding: 10px;
  font-size: 15px;
}

.activ-nav .index{
  width: 20%;
  height: 60rpx;
  line-height: 60rpx;
}

.activ-nav .name{
  width: 50%;
  height: 60rpx;
  padding-left: 5px;
  line-height: 60rpx;
}

.activ-nav .videoCount{
  width: 30%;
  height: 60rpx;
  line-height: 60rpx;
  color:#FA897B;
  text-align: center;
}




